#include <bits/stdc++.h>

using namespace std;


int main(){
    
    int s,t,a,b, m ,n,d;
    cin>>s>>t>>a>>b>>m>>n;
     
     int applecount=0;
     for(int i=0;i<m;i++){
         cin>>d;
         if(d+a>=s&& d+a<=t){
             applecount++;
         }
     }
     int mangocount=0;
     for(int i=0;i<n;i++){
         cin>>d;
         if(d+b<=t&&d+b>=s){
             mangocount++;
             
         }
     }
  
  cout<<applecount<<endl<<mangocount;
    
    
    return 0;
}
